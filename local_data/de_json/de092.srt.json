      "courseNo" : 2,
      "timeline" : "00:01:28,533 --> 00:01:29,850",
      "english" : "What color is that?"
   },
   {
      "lessonNo" : 92,
      "chinese" : "绿色.",
      "sentenceNo" : 28,
      "courseNo" : 2,
      "timeline" : "00:01:30,065 --> 00:01:31,035",
