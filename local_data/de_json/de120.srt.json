[
   {
      "lessonNo" : 120,
      "chinese" : "Hey you. 你把衣服留在供干机里了.",
      "sentenceNo" : 1,
      "courseNo" : 2,
      "timeline" : "00:00:07,984 --> 00:00:11,712",
      "english" : "Hey you. You left laundry in the dryer."
   },
   {
      "lessonNo" : 120,
      "chinese" : "你应该自己把这事从头至尾的全部做完.",
      "sentenceNo" : 2,
      "courseNo" : 2,
      "timeline" : "00:00:12,537 --> 00:00:15,423",
      "english" : "You're supposed to do the whole load yourself- start to finish."
   },
   {
      "lessonNo" : 120,
      "chinese" : "我忘了. 对不起.",
      "sentenceNo" : 3,
      "courseNo" : 2,
      "timeline" : "00:00:16,138 --> 00:00:18,301",
      "english" : "I forgot. I'm sorry."
   },
   {
