[
   {
      "lessonNo" : "08--1",
      "sentenceNo" : 1,
      "english" : "Molly, I need your special talent for handling special matters."
   },
   {
      "lessonNo" : "08--1",
      "sentenceNo" : 2,
      "english" : "Like what special matters?"
   },
   {
