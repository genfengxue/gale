   {
      "lessonNo" : "08--1",
      "sentenceNo" : 4,
      "english" : "Yes?"
   },
   {
      "lessonNo" : "08--1",
      "sentenceNo" : 5,
      "english" : "I have three tonsillectomies set for Friday with Dr. Earl."
   },
   {
      "lessonNo" : "08--1",
      "sentenceNo" : 6,
      "english" : "Yes?"
